<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="com.insatoulouse.chatsystem.gui.MessageRow">
  <grid id="27dc6" binding="panel1" default-binding="true" layout-manager="BorderLayout" hgap="0" vgap="0">
    <constraints>
      <xy x="20" y="20" width="560" height="400"/>
    </constraints>
    <properties>
      <background color="-1"/>
      <foreground color="-1"/>
    </properties>
    <border type="empty">
      <size top="10" left="10" bottom="10" right="50"/>
    </border>
    <children>
      <component id="6b980" class="javax.swing.JTextArea" binding="textMessage">
        <constraints border-constraint="Center"/>
        <properties>
          <background color="-26215"/>
          <lineWrap value="true"/>
          <margin top="5" left="5" bottom="5" right="5"/>
          <wrapStyleWord value="true"/>
        </properties>
      </component>
      <grid id="b8730" layout-manager="GridLayoutManager" row-count="1" column-count="2" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="-1">
        <margin top="0" left="0" bottom="0" right="0"/>
        <constraints border-constraint="South"/>
