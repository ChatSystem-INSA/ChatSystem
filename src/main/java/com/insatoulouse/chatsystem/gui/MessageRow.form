              <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties>
              <enabled value="true"/>
              <selected value="false"/>
              <text value="Enregistrer le fichier"/>
              <visible value="true"/>
            </properties>
          </component>
          <hspacer id="1b531">
